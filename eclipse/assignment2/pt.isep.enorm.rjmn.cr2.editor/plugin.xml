            id="pt.isep.enorm.atb.cr2.presentation.Cr2EditorID"
            name="%_UI_Cr2Editor_label"
            icon="icons/full/obj16/Cr2ModelFile.gif"
            extensions="cr2"
            class="pt.isep.enorm.atb.cr2.presentation.Cr2Editor"
            contributorClass="pt.isep.enorm.atb.cr2.presentation.Cr2ActionBarContributor">
