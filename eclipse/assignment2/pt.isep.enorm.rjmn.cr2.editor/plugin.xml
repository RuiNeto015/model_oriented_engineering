      <category
            id="org.eclipse.emf.ecore.Wizard.category.ID"
            name="%_UI_Wizard_category"/>
      <wizard
            id="pt.isep.enorm.atb.cr2.presentation.Cr2ModelWizardID"
            name="%_UI_Cr2ModelWizard_label"
            class="pt.isep.enorm.atb.cr2.presentation.Cr2ModelWizard"
